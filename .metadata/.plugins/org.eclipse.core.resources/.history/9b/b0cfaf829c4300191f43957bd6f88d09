package threadSychornized;

public class main {
	
	public static void main(String[] args) {
		
		
		MyThread runable = new MyThread();
		
		Thread mythread1 =  new Thread(runable);
		Thread mythread2 =  new Thread(runable);
		Thread mythread3 =  new Thread(runable);
		
		mythread1.setName("mythread1");
		mythread2.setName("mythread2");
		mythread3.setName("mythread3");
		
		mythread1.start();
		mythread2.start();
		mythread3.start();
		
		
	}

}
