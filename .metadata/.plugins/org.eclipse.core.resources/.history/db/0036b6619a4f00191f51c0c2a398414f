package test1;

import java.io.File;
import java.io.IOException;

import jxl.Cell;
import jxl.Sheet;
import jxl.Workbook;
import jxl.read.biff.BiffException;

public class test1 {
	
	
	public static void main(String[] args) throws BiffException, IOException {
		
		
		
		Workbook test1work = Workbook.getWorkbook(
				new File("C:\\Users\\Administrator\\Desktop\\workbooktest\\33334.xls"));
		
		
		Sheet test1 = test1work.getSheet("3333");
		
		
		int rows = test1.getRows();
		int columns   =  test1.getColumns();
		
		
		System.out.println(rows);
		System.out.println(columns);
		
		for (int i = 0; i < columns+1; i++) {
			
			Cell testcell =  test1.getCell(i, 0);
			
			if (testcell !=null) {
				System.out.println(test1.getCell(i, 0).getContents());
				
			}
			else {
				System.out.println("null");
			}
			
		}
		
		
		
		
		
		
		
	}

}
